    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-plugin-import": "^2.24.2",
    "jest": "^27.2.5",
    "nodemon": "^2.0.13",
    "supertest": "^6.1.6",
    "swagger-ui": "^3.52.3",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
