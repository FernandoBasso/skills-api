    "lint": "NODE_ENV=lint eslint . --ext .ts",
    "db:devel:setup": "mongo config/db-devel-setup.js",
    "db:devel:seed": "mongo config/db-devel-seed.js",
    "db:test:setup": "mongo config/db-test-setup.js"
