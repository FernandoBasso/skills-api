{
  "name": "express-getting-started",
  "description": "Getting started tutorial from the docs (with TypeScript)",
  "version": "0.0.1",
  "main": "src/index.ts",
  "type": "commonjs",
  "scripts": {
    "api": "NODE_PATH=. NODE_ENV=devel nodemon src/server.ts",
